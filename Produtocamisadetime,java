public class CamisaTime extends Produto {
    private String time;

    public CamisaTime(String nome, double preco, String tipo, double peso, String codigoBarras, String codigoIdentificacao, String time) {
        super(nome, preco, tipo, peso, codigoBarras, codigoIdentificacao);
        this.time = time;
    }

    @Override
    public String toString() {
        return super.toString() + " | Time: " + time;
    }
}
